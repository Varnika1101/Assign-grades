#include<stdio.h>
void main()
{
	float marks;
	printf("Enter your marks out of 100:");
	scanf("%f",&marks);
	if(marks>=85)
	  printf("You got Grade A.\n");
	else if(marks>=70)
	  printf("You got Grade B.\n");
	else if(marks>=55)
	  printf("You got Grade C.\n");
	else if(marks>=40)
	  printf("You got Grade D.\n");
	else
	  printf("You got Grade F.\n");
}
